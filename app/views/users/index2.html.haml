.users-wrapper
  = render 'header'
  .users-wrapper__contents
    %p アカウントをお持ちでない方はこちら
    = link_to "#" do
      新規会員登録
    .users-wrapper__contents__sign
      = link_to "#" do
        .users-wrapper__contents__sign__btn.facebook
          %i.fa.fa-facebook-square
          %span Facebookでログイン
      = link_to "#" do
        .users-wrapper__contents__sign__btn.google
          %i.fa.fa-google
          %span Googleでログイン
    .users-wrapper__contents__signin
      = form_with(local: true) do |f|
        = f.text_field :email, placeholder: "メールアドレス", class: "textfield-default"
        = f.text_field :passwors, placeholder: "パスワード", class: "textfield-default"
        = image_tag "#{asset_path('dummy_recaptcha.png')}", alt: 'dummy', height: '', width: '200px', class: ''
  = render 'footer'